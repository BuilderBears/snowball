<!DOCTYPE html>
<html>
<head>
  <title>GuideMe</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrap">
    <%= render 'layouts/header' %>

    <div class="container top-spacer" id="main">
      <%= flash_messages %>
      <%= yield %>
    </div>
  </div>

  <%= render 'layouts/footer' %>

</body>
</html>
